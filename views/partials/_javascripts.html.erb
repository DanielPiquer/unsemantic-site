<script>
(function(w, d){
  var fusion;
  var host = w.location.hostname;

  if (!host.match('unsemantic.com')) {
    return;
  }

  d.getElementById('fusion_ad').style.display = 'block';

  fusion = d.createElement('script');
  fusion.src = w.location.protocol + '//adn.fusionads.net/api/1.0/ad.js?zoneid=222&rand=' + Math.floor(Math.random() * 9999999);
  fusion.async = true;
  (d.head || d.getElementsByTagName('head')[0]).appendChild(fusion);
})(this, this.document);
</script>